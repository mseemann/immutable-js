{
  "name": "immutable",
  "version": "3.8.1",
  "description": "Immutable Data Collections",
  "homepage": "https://facebook.github.com/immutable-js",
  "author": {
    "name": "Lee Byron",
    "url": "https://github.com/leebyron"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/facebook/immutable-js.git"
  },
  "bugs": {
    "url": "https://github.com/facebook/immutable-js/issues"
  },
  "main": "dist/immutable.js",
  "typings": "dist/immutable-nonambient.d.ts",
  "typescript": {
    "definition": "dist/immutable.d.ts"
  },
  "scripts": {
    "build": "grunt default && gulp default",
    "lint": "eslint src/ && grunt lint && gulp lint",
    "testonly": "./resources/node_test.sh",
    "test": "npm run lint && npm run testonly",
    "perf": "node ./resources/bench.js",
    "start": "npm run build && node ./pages/resources/start.js",
    "deploy": "(cd ./pages/out && git init && git config user.name \"Travis CI\" && git config user.email \"github@fb.com\" && git add . && git commit -m \"Deploy to GitHub Pages\" && git push --force --quiet \"https://${GH_TOKEN}@github.com/facebook/immutable-js.git\" master:gh-pages > /dev/null 2>1)"
  },
  "jest": {
    "scriptPreprocessor": "resources/jestPreprocessor.js",
    "testFileExtensions": [
      "js",
      "ts"
    ],
    "persistModuleRegistryBetweenSpecs": true
  },
  "devDependencies": {
    "acorn": "~4.0.3",
    "babel-eslint": "^7.1.0",
    "benchmark": "^2.1.2",
    "bluebird": "3.4.6",
    "browser-sync": "2.17.6",
    "browserify": "^13.1.1",
    "colors": "1.1.2",
    "del": "2.2.2",
    "es6-transpiler": "0.7.18",
    "eslint": "^3.10.1",
    "estraverse": "4.2.0",
    "express": "^4.13.4",
    "fbjs-scripts": "^0.7.1",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-jshint": "1.0.0",
    "grunt-release": "0.14.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-filter": "4.0.0",
    "gulp-header": "1.8.8",
    "gulp-jest": "^0.6.0",
    "gulp-jshint": "^2.0.4",
    "gulp-less": "3.3.0",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "2.2.0",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "harmonize": "1.4.4",
    "jasmine-check": "^0.1.2",
    "jest-cli": "^17.0.1",
    "jshint-stylish": "^2.2.1",
    "magic-string": "0.16.0",
    "marked": "0.3.6",
    "microtime": "^2.0.0",
    "node-jsx": "^0.13.3",
    "react": "^15.3.2",
    "react-router": "^3.0.0",
    "react-tools": "^0.13.3",
    "rollup": "0.36.3",
    "run-sequence": "1.2.2",
    "through2": "2.0.1",
    "typescript": "2.0.9",
    "uglify-js": "2.7.4",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "dist",
    "contrib",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "keywords": [
    "immutable",
    "persistent",
    "lazy",
    "data",
    "datastructure",
    "functional",
    "collection",
    "stateless",
    "sequence",
    "iteration"
  ],
  "license": "BSD-3-Clause"
}
